{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "root": true,
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "react/forbid-prop-types": "off",
    "react/jsx-filename-extension": "off",
    "object-curly-spacing": ["error", "never"],
    "no-console": "off",
    "no-plusplus": "off",
    "no-case-declarations": "off",
    "no-unused-vars": "warn"
  }
}
